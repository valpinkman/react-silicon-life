{
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:jest/recommended"
  ],
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "rules": {
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "react/forbid-prop-types": 0,
    "react/sort-comp": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "jsx-a11y/anchor-is-valid": 0
  }
}
